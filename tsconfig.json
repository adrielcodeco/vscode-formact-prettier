{
  "compilerOptions": {
    "types": ["node"],
    "declaration": true,
    "declarationMap": true,
    "diagnostics": true,
    "stripInternal": true,
    "alwaysStrict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "downlevelIteration": true,
    "noFallthroughCasesInSwitch": true,
    "removeComments": true,
    "preserveConstEnums": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "checkJs": false,
    "allowJs": false,
    "strict": true,
    "pretty": true,
    "noEmit": true,
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "lib": ["es2015"],
    "baseUrl": ".",
    "paths": {
      "#/*": ["src/*"]
    }
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
